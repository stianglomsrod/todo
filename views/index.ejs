<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Todo App</title>
  </head>
  <body>
    <a href="/addtask">Add New Task</a>
    <ul>
      <% tasks.forEach((task) => { %>
      <li><%= task.task %> - <%= task.timestamp %></li>
      <% }); %>
    </ul>
  </body>
</html>
